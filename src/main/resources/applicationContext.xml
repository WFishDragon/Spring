<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="class" class="pojo.Classes">
        <property name="id" value="0"></property>
        <property name="name" value="一班"></property>
    </bean>
    <bean id="stu" class="pojo.Student">
        <property name="id" value="0"></property>
        <property name="name" value="王昱龙"></property>
        <property name="classes" ref="class"></property>
    </bean>
</beans>
